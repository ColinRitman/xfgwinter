{
  "git_configuration": {
    "primary_repo": {
      "name": "xfgwin",
      "url": "https://github.com/ColinRitman/xfgwin.git",
      "branch": "main",
      "remote_name": "xfgwin"
    },
    "project_repo": {
      "name": "xfg-stark-agent-coordination",
      "url": "https://github.com/ColinRitman/COLDL3.git",
      "branch": "master",
      "remote_name": "origin"
    },
    "git_root": {
      "path": "/Users/aejt/fuegowalletproof/xfg-stark-agent-coordination",
      "is_primary_workspace": true,
      "background_agents_path": "background-agents"
    }
  },
  "repository_management": {
    "sync_strategy": "background_agents_to_xfgwin",
    "commit_patterns": {
      "background_agents": "feat(background-agents):",
      "xfg_stark": "feat(xfg-stark):",
      "coordination": "feat(coordination):",
      "security": "feat(security):"
    },
    "branch_strategy": {
      "development": "develop",
      "feature": "feature/",
      "hotfix": "hotfix/",
      "release": "release/"
    }
  },
  "background_agents_git": {
    "enabled": true,
    "auto_commit": true,
    "auto_push": false,
    "commit_interval": 3600,
    "sync_to_xfgwin": true,
    "sync_interval": 7200,
    "include_patterns": [
      "background-agents/**/*",
      "config/**/*",
      "logs/**/*",
      "*.md",
      "*.json",
      "*.py"
    ],
    "exclude_patterns": [
      "**/__pycache__/**",
      "**/*.pyc",
      "**/venv/**",
      "**/node_modules/**",
      "**/.env",
      "**/logs/*.log"
    ]
  },
  "xfgwin_integration": {
    "target_repo": "xfgwin",
    "target_branch": "main",
    "sync_method": "subtree_push",
    "maintain_history": true,
    "force_push": false,
    "sync_frequency": "daily",
    "backup_before_sync": true
  },
  "monitoring": {
    "git_status_monitoring": true,
    "commit_health_check": true,
    "branch_protection": true,
    "merge_conflict_detection": true,
    "sync_status_tracking": true
  },
  "notifications": {
    "git_events": {
      "commit": true,
      "push": true,
      "merge": true,
      "conflict": true,
      "sync": true
    },
    "channels": {
      "console": true,
      "slack": true,
      "email": false
    }
  }
}
